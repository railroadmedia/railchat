{
  "name": "railroad/railchat",
  "type": "library",
  "description": "Wrapper package to integrate getstream/stream-php library into railroad platforms",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "get-stream/stream": "^5.1",
    "laravel/framework": "^9.9",
    "railroad/permissions": "^2.0",
    "get-stream/stream-chat": "^2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Railchat\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railchat\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
