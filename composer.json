{
  "name": "railroad/railchat",
  "type": "library",
  "description": "Wrapper package to integrate getstream/stream-php library into railroad platforms",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "~7.1|^8.0",
    "get-stream/stream": "^4.1",
    "laravel/framework": "5.6.*",
    "railroad/permissions": "^1.0",
    "get-stream/stream-chat": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Railchat\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railchat\\Tests\\": "tests/"
    }
  }
}
